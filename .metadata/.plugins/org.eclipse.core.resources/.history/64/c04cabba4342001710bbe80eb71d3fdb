		Dictionary example = new Dictionary();
		System.out.println(String.format("%-30s%-20s%s","Operation","Output","Dictiooary"));
		System.out.println("---------------------------------------------------------------------------");
		System.out.print(String.format("%-30s","insert(5,A)"));
		example.insert(5, "A");
		example.print();
		System.out.print(String.format("%-30s","insert(7,B)"));
		example.insert(7, "B");
		example.print();
		System.out.print(String.format("%-30s","insert(2,C)"));
		example.insert(2, "C");
		example.print();
		System.out.print(String.format("%-30s","insert(8,D)"));
		example.insert(8, "D");
